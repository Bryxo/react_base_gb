{
  "name": "my-app",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://bryxo.github.io/react_base_gb/",
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.6.1",
    "html-webpack-plugin": "^5.5.0",
    "prop-types": "^15.8.1",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.3.0",
    "sass": "^1.50.0",
    "sass-loader": "^12.6.0"
  },
  "scripts": {
    "start": "webpack serve --mode=development",
    "build": "webpack --mode=production",
    "lint": "npx tsc --noEmit && npx eslint --ext .js,.jsx,.ts,.tsx --fix ./",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "jest && npm run loki:test",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --colors",
    "loki:update": "npm run build-storybook && loki update --requireReference --reactUri file:./storybook-static",
    "loki:approve": "loki approve",
    "loki:test": "npm run build-storybook && loki test --requireReference --reactUri file:./storybook-static"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.24",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "babel-loader": "^8.2.4",
    "css-loader": "^6.7.1",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-storybook": "^0.5.8",
    "file-loader": "^6.2.0",
    "gh-pages": "^3.2.3",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "nanoid": "^3.3.3",
    "prettier": "^2.6.2",
    "style-loader": "^3.3.1",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run lint"
    }
  }
}
